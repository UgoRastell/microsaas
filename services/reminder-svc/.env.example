# Node Environment
NODE_ENV=development

# NATS
NATS_URL=nats://localhost:4222

# Database
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Reminder Settings
# Cron expression for invoice check job (default: every day at 10:00 AM)
REMINDER_CRON=0 10 * * *
# Days after due date when to send first reminder
REMINDER_FIRST_DAYS=3
# Days after due date when to send second reminder
REMINDER_SECOND_DAYS=7
# Days after due date when to send final reminder
REMINDER_FINAL_DAYS=14
